{
  "name": "api",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": ">=12",
    "npm": ">=6"
  },
  "scripts": {
    "lint": "eslint src/**/*.js",
    "test": "BABEL_DISABLE_CACHE=1 TEST=1 node -r esm jest-config/index.js",
    "start": "npm run start:aws",
    "start:aws": "NODE_ENV=development && SLS_CLOUD=aws && sls offline --config ./serverless-aws.yml --env aws",
    "start:azure": "NODE_ENV=development && SLS_CLOUD=azure && sls offline --config ./serverless-azure.yml --env azure",
    "deploy": "npm run deploy:azure && npm run deploy:aws",
    "deploy:aws": "NODE_ENV=production && SLS_CLOUD=aws && sls deploy --verbose --config serverless-aws.yml --env aws",
    "deploy:azure": "NODE_ENV=production && SLS_CLOUD=azure && sls deploy --verbose --config serverless-azure.yml --env azure",
    "remove": "npm run remove:azure && npm run remove:aws",
    "remove:aws": "NODE_ENV=production && SLS_CLOUD=aws && sls remove --force --config serverless-aws.yml --env aws",
    "remove:azure": "NODE_ENV=production && SLS_CLOUD=azure && sls remove --force --config serverless-azure.yml --env azure"
  },
  "dependencies": {
    "bl": "^4.0.2",
    "mongodb": "^3.5.9",
    "mongodb-client-encryption": "^1.1.0",
    "uuid": "3.3.2"
  },
  "devDependencies": {
    "@babel/core": "7.2.2",
    "@babel/plugin-proposal-object-rest-spread": "7.2.0",
    "@babel/plugin-syntax-dynamic-import": "7.2.0",
    "@babel/plugin-transform-async-to-generator": "7.2.0",
    "@babel/plugin-transform-exponentiation-operator": "7.2.0",
    "@babel/preset-env": "7.3.1",
    "aws-sdk": "^2.620.0",
    "babel-jest": "24.0.0",
    "babel-loader": "8.0.5",
    "babel-plugin-module-resolver": "3.1.3",
    "env-cmd": "8.0.2",
    "eslint": "^7.4.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "esm": "3.1.0",
    "jest": "24.0.0",
    "prettier": "^2.0.5",
    "serverless": "^1.75.0",
    "serverless-azure-functions": "^2.0.0",
    "serverless-dotenv-plugin": "^2.4.2",
    "serverless-offline": "^6.4.0",
    "serverless-webpack": "^5.3.2",
    "webpack": "^4.42.0",
    "webpack-monitor": "1.0.14",
    "webpack-node-externals": "1.7.2",
    "webpack-strip-block": "^0.2.0"
  }
}
