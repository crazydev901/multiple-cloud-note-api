{
  "name": "api",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": ">=12",
    "npm": ">=6"
  },
  "scripts": {
    "lint": "eslint src/**/*.js",
    "test": "BABEL_DISABLE_CACHE=1 TEST=1 node -r esm jest-config/index.js",
    "sls": "run(){ node -r esm node_modules/.bin/sls $1; }; run",
    "start": "npm run start:aws",
    "start:aws": "npm run sls 'offline start --watch --config serverless-aws.yml'",
    "start:azure": "npm run sls 'offline start --watch --config serverless-azure.yml'",
    "deploy": "npm run deploy:azure && npm run deploy:aws",
    "deploy:aws": "npm run sls 'deploy --verbose --config serverless-aws.yml'",
    "deploy:azure": "npm run sls 'deploy --verbose --config serverless-azure.yml'"
  },
  "dependencies": {
    "stripe": "8.32.0",
    "uuid": "3.3.2"
  },
  "devDependencies": {
    "@babel/core": "7.2.2",
    "@babel/plugin-proposal-object-rest-spread": "7.2.0",
    "@babel/plugin-syntax-dynamic-import": "7.2.0",
    "@babel/plugin-transform-async-to-generator": "7.2.0",
    "@babel/plugin-transform-exponentiation-operator": "7.2.0",
    "@babel/preset-env": "7.3.1",
    "aws-sdk": "^2.620.0",
    "babel-jest": "24.0.0",
    "babel-loader": "8.0.5",
    "babel-plugin-module-resolver": "3.1.3",
    "env-cmd": "8.0.2",
    "esm": "3.0.84",
    "eslint": "^7.4.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "jest": "24.0.0",
    "prettier": "^2.0.5",
    "serverless": "^1.75.0",
    "serverless-dotenv-plugin": "^2.4.2",
    "serverless-offline": "^6.4.0",
    "serverless-webpack": "^5.3.2",
    "webpack": "^4.42.0",
    "webpack-monitor": "1.0.14",
    "webpack-node-externals": "1.7.2"
  }
}
